<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="S_BroadcastEnterGame">
		<int name="playerId"/>
		<float name="posX"/>
		<float name="posY"/>
		<float name="posZ"/>
		<float name="rotationX"/>
		<float name="rotationY"/>
		<float name="rotationZ"/>
	</packet>
	<packet name ="C_LeaveGame">
	</packet>
	<packet name ="S_BroadcastLeaveGame">
		<int name="playerId"/>
	</packet>
	<packet name ="S_PlayerList">
		<list name="player">
			<bool name ="isSelf"/>
			<int name="playerId"/>
			<float name="posX"/>
			<float name="posY"/>
			<float name="posZ"/>
			<float name="rotationX"/>
			<float name="rotationY"/>
			<float name="rotationZ"/>
		</list>
	</packet>
	<packet name="C_Move">
		<float name="posX"/>
		<float name="posY"/>
		<float name="posZ"/>
		<float name="rotationX"/>
		<float name="rotationY"/>
		<float name="rotationZ"/>
	</packet>
	<packet name ="S_BroadcastMove">
		<int name="playerId"/>
		<float name="posX"/>
		<float name="posY"/>
		<float name="posZ"/>
		<float name="rotationX"/>
		<float name="rotationY"/>
		<float name="rotationZ"/>
	</packet>
</PDL>